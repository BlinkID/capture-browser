{
  "name": "capture-main",
  "description": "",
  "private": true,
  "version": "1.2.1",
  "author": "",
  "scripts": {
    "build": "concurrently pnpm:build:js pnpm:build:types",
    "build:dev": "concurrently pnpm:build:js-dev pnpm:build:types",
    "build:js": "vite build",
    "build:js-dev": "vite build --mode development",
    "build:types": "tsc",
    "clean": "rimraf .turbo types dist public/resources/* *.tsbuildinfo",
    "lint": "concurrently pnpm:lint:types pnpm:lint:eslint",
    "lint:eslint": "eslint ./src --max-warnings 0",
    "lint:types": "tsc --emitDeclarationOnly false --noEmit",
    "test": "vitest",
    "watch": "concurrently pnpm:watch:js pnpm:watch:types",
    "watch:js": "pnpm run build:js --watch --mode development",
    "watch:types": "pnpm run build:types --watch"
  },
  "type": "module",
  "main": "./dist/capture.umd.cjs",
  "module": "./dist/capture.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/capture.js",
      "require": "./dist/capture.umd.cjs"
    },
    "./style.css": "./dist/style.css",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "@kobalte/core": "^0.11.0",
    "@mb/utils": "workspace:*",
    "@motionone/solid": "^10.16.4",
    "@solid-primitives/keyed": "^1.2.0",
    "@wessberg/connection-observer": "^1.0.5",
    "capture-wasm": "workspace:*",
    "capture-worker": "workspace:*",
    "clsx": "^2.0.0",
    "comlink": "^4.4.1",
    "common-tags": "^1.8.2",
    "deepmerge": "^4.3.1",
    "is-plain-obj": "^4.1.0",
    "nanoid": "^5.0.2",
    "rvfc-polyfill": "^1.0.7",
    "solid-js": "^1.8.3",
    "solid-slider": "^1.3.15",
    "solid-zustand": "^1.7.0",
    "tiny-invariant": "^1.3.1",
    "ts-pattern": "^5.0.5",
    "ua-parser-js": "^1.0.36",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@acab/ecsstatic": "^0.7.1",
    "@styled/typescript-styled-plugin": "^1.0.0",
    "@types/common-tags": "^1.8.3",
    "@types/emscripten": "^1.39.9",
    "@types/ua-parser-js": "^0.7.38",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-solid": "^0.13.0",
    "happy-dom": "^12.9.1",
    "rimraf": "^5.0.5",
    "sass": "^1.69.4",
    "tsconfig": "workspace:*",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-solid": "^2.7.2",
    "vite-plugin-solid-svg": "^0.7.0",
    "vitest": "^0.34.6",
    "zx": "^7.2.3"
  },
  "keywords": [],
  "license": "ISC"
}
